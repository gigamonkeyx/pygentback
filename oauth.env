# OAuth Configuration for PyGent Factory
# Copy this file to oauth.env and fill in your OAuth application credentials

# Cloudflare OAuth (for MCP servers)
CLOUDFLARE_CLIENT_ID=your_cloudflare_client_id_here
CLOUDFLARE_CLIENT_SECRET=your_cloudflare_client_secret_here

# GitHub OAuth (for GitHub MCP server)
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# Google OAuth (for Google services)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Microsoft OAuth (for Microsoft services)
MICROSOFT_CLIENT_ID=your_microsoft_client_id_here
MICROSOFT_CLIENT_SECRET=your_microsoft_client_secret_here

# Slack OAuth (for Slack integration)
SLACK_CLIENT_ID=your_slack_client_id_here
SLACK_CLIENT_SECRET=your_slack_client_secret_here

# Discord OAuth (for Discord integration)
DISCORD_CLIENT_ID=your_discord_client_id_here
DISCORD_CLIENT_SECRET=your_discord_client_secret_here

# Notion OAuth (for Notion integration)
NOTION_CLIENT_ID=your_notion_client_id_here
NOTION_CLIENT_SECRET=your_notion_client_secret_here

# Linear OAuth (for Linear integration)
LINEAR_CLIENT_ID=your_linear_client_id_here
LINEAR_CLIENT_SECRET=your_linear_client_secret_here
