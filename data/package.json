{
  "name": "pygent-factory",
  "version": "1.0.0",
  "description": "PyGent Factory - Advanced AI Reasoning System",
  "type": "module",
  "scripts": {
    "dev:frontend": "cd src/ui && npm run dev",
    "dev:backend": "python -m src.api.main",
    "dev:backend:minimal": "python -m src.api.main_minimal",
    "dev:backend:simple": "python -m src.api.main_simple",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:minimal": "concurrently \"npm run dev:backend:minimal\" \"npm run dev:frontend\"",
    "build:frontend": "cd src/ui && npm run build",
    "install:frontend": "cd src/ui && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.0"
  }
}
